<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>Uploaded CSV Files</title>
  <link rel="stylesheet" href="css/fileStyle.css">
</head>
<body>
  <h1>Uploaded CSV Files</h1>
  <table>
    <thead>
      <tr>
        <th>File Name</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% files.forEach(function(file) { %>
        <tr>
          <td><a href="/files/<%= file %>"><%= file %></a></td>
          <td>
            <form action="/files/<%= file %>?_method=DELETE" method="POST">
              <button type="submit" class="delete-btn">Delete</button>
            </form>
          </td>
        </tr>
      <% }); %>
    </tbody>
  </table>
  <br>
    <button onclick="location.href='/'" class="btn">Go to Upload Page</button>
</body>
</html>
